{
    "extends": "@tsconfig/svelte/tsconfig.json",
    "include": [
        "src/**/*",
        "src/node_modules"
    ],
    "exclude": [
        "node_modules/*",
        "config/*",
        "assets/*"
    ],
    "compilerOptions": {
        "allowSyntheticDefaultImports":true,
        "moduleResolution": "node",
        "sourceMap": true,
        "target": "es2020",
        "types": ["svelte"]
    }
}