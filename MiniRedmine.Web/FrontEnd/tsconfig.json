{
  "extends": "@tsconfig/svelte/tsconfig.json",
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules/**/*",
    "config/**/*",
    "assets/**/*"
  ],
  "compilerOptions": {
    "noImplicitAny": false,
    "sourceMap": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "target": "es6",
    "lib": [ "es6", "dom" ],
    "types": [ "svelte" ]
  }
}